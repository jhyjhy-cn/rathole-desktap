{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    "shell:default",
    "dialog:default",
    "process:default",
    "http:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "rathole",
          "cmd": "rathole",
          "args": true,
          "sidecar": false
        }
      ]
    },
    "shell:allow-kill"
  ]
}
